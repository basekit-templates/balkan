<!-- Header include -->
<header class="template-header">
    <div class="company-info">
        {{widget('logo', 'logo', {"defaultlogo": asset('images/logo-default.png')})|raw}}
        {{widget('companyname', 'companyname', {"business": "profile"})|raw}}
    </div>

    {{widget('profile', 'profile', {
        'profileType':'socialicons',
        'socialIconsTwitter':'profile',
        'socialIconsFacebook':'profile',
        'socialIconsGoogleplus':'profile',
        'socialIconsYoutube':'profile',
        'socialIconsLinkedin':'profile',
      })|raw}}
</header>